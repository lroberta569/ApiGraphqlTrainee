type Car {
    id: ID!,
    licensePlate: String,
    name: String,
    manufactureDate: String,
    version: Int
}

input CarInput {
    id: ID
    name: String
    licensePlate: String
    manufactureDate: String
    version: Int
}
type Query{
    findAllCar: [Car]
    findCarById(id: ID ): Car
    generatePdfABase64(id: ID):String

}
type Mutation{
    createCar(car: CarInput):Car
    updateCar(car : CarInput):Car
    deleteCar(id : ID) : String
}